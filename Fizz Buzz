public class FizzBuzz{
    public static int[] solution(int number) {
      int A = 0, B = 0, C = 0;
      for(int i = 3; i < number; i = i + 3) if (i % 5 != 0) A++;
      for(int i = 5; i < number; i = i + 5) if (i % 3 != 0) B++;
      for(int i = 15; i < number; i = i + 15) C++;
      return new int[]  {A, B, C};
    }
}
